<div class="container first-container">
  <div class="row">

    <div class="col-md-8 col-md-offset-2">

      <form id="form-register" class="form-horizontal form-register">
        <div class="form-group">
          <div class="col-md-10 col-md-offset-2"><h1>新用户注册</h1></div>
        </div>
        {{#parseJSON '{"id": "user_name", "label": "用户名", "horizontal": true}'}}
          {{> form-group }}
        {{/parseJSON}}
        {{#parseJSON '{"id": "password", "label": "密码", "horizontal": true, "type":"password"}'}}
          {{> form-group }}
        {{/parseJSON}}
        {{#parseJSON '{"id": "password_rp", "label": "重复密码", "horizontal": true, "type":"password"}'}}
          {{> form-group }}
        {{/parseJSON}}
        {{#parseJSON '{"id": "email", "label": "电子邮箱", "horizontal": true, "type":"email"}'}}
          {{> form-group }}
        {{/parseJSON}}
        <div class="form-group">
          <div class="col-md-10 col-md-offset-2">
            <label class="checkbox" for="reg_eula">
              <input type="checkbox" id="reg_eula" name="reg_eula"> 我接受<a href="#">最终用户许可协议</a>。
            </label>
          </div>
        </div>
        <div class="form-group">
          <div class="col-md-10 col-md-offset-2">
            <a href="register_details.html" class="btn btn-primary btn-lg">注册</a> <a href="#form-login" data-toggle="tab" class="btn btn-lg btn-default">返回</a>
          </div> 
        </div>
      </form>
    </div>
  </div>
</div>